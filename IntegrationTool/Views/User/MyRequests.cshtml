
@{
    ViewData["Title"] = "MyRequest";
    Layout = "~/Views/Shared/_Partner.cshtml";
}
@section JavaScript
{
    <script>
        $("#sidenav li").each(function () {
            if ($(this).data("link") == "my_request") {
                $(this).addClass("active");
            }
            else {
                $(this).removeClass("active");
            }
        });
    </script>
}
<h1 class="page-header">My Requests</h1>

<div>

    <table class="table table-condensed table-striped custom-table">
        <thead>
            <tr>
                <th colspan="3">Active</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>Add Partner 1</td>
                <td>For Approval - CTIPD</td>
                <td><a class=" btn btn-default" href="/User/ForApprovals" ><span class="glyphicon glyphicon-eye-open mr-5"></span>Details</a></td>
            </tr>
            <tr>
                <td>Edit Partner 2</td>
                <td>For Approval - CTIPD</td>
                <td><a class=" btn btn-default" href="/User/ForApprovals" ><span class="glyphicon glyphicon-eye-open mr-5"></span>Details</a></td>
            </tr>
        </tbody>
    </table>
